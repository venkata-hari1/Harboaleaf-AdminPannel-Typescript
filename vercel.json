{
    "headers": [
      {
        "source": "/api/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://harboaleaf-admin-pannel-typescript.vercel.app" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
          { "key": "Access-Control-Allow-Credentials", "value": "true" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "http://34.234.64.81/api/:path*"
      }
    ]
  }